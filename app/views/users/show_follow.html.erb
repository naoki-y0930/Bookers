  <div class="container">
   <div class="row">
    <div class="col-md-8 col-md-offset-2">
     <h1 class="editbook">Followユーザー一覧</h1>
       <table class="table table-hover">
        <thead>
         <tr>
          <th>name</th>
          <th>show</th>
         </tr>
        </thead>
        <tbody>
       <% @user.followings.each do |user| %>
        <tr>
         <td><%= user.name %></td>
         <td><%= link_to 'show', user_path(user) %></td>
        </tr>
       <% end %>
      </tbody>
     </table>
    </div>
   <div class="col-md-2"></div>
  </div>
 </div>
